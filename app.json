{
  "expo": {
    "name": "GrabSnap",
    "slug": "grabsnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-grabsnap.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-grabsnap.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grabsnap.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon-grabsnap.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.grabsnap.app"
    },
    "web": {
      "favicon": "./assets/favicon-grabsnap.png"
    },
    "plugin": ["expo-font"]
  }
}
